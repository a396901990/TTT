<!--https://github.com/chrisbanes/ActionBar-PullToRefresh-->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.dean.travltotibet.ui.LoadMoreListView
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:id="@+id/article_fragment_list_rv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@null"
        android:footerDividersEnabled="false"
        android:headerDividersEnabled="false"
        android:scrollbars="none"></com.dean.travltotibet.ui.LoadMoreListView>


    <include
        android:id="@+id/no_result_content"
        layout="@layout/no_result_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="top" />
</FrameLayout>
