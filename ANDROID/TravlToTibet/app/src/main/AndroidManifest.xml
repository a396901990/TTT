<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dean.travltotibet">

    <permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".TTTApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/Theme.AppBaseTheme"
        android:label="@string/app_name">
        <activity
            android:name=".activity.RouteActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.GuideRouteActivity"
            android:label="@string/title_activity_guide_route"
            android:theme="@style/Theme.AppBaseTheme"></activity>
    </application>

</manifest>
